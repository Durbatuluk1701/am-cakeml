(** val firstn : nat -> 'a1 list -> 'a1 list **)

fun firstn n l =
  case n of
    O => []
  | S n0 => (case l of
               [] => []
             | a :: l0 => a :: (firstn n0 l0))

(** val skipn : nat -> 'a1 list -> 'a1 list **)

fun skipn n l =
  case n of
    O => l
  | S n0 => (case l of
               [] => []
             | _ :: l0 => skipn n0 l0)
