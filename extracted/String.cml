datatype string =
  EmptyString 
| String ascii string

(** val eqb : string -> string -> bool **)

fun eqb s1 s2 =
  case s1 of
    EmptyString => (case s2 of
                      EmptyString => True
                    | String _ _ => False)
  | String c1 s1' =>
    (case s2 of
       EmptyString => False
     | String c2 s2' =>
       (case Ascii.eqb c1 c2 of
          True => eqb s1' s2'
        | False => False))
